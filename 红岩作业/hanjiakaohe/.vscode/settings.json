  "editor.suggestSelection": "first",
  "editor.wordBasedSuggestions": "matchingDocuments",
  "files.associations": {
    "*.js": "javascript",
    "*.html": "html",
    "*.css": "css"
},
"liveServer.settings.port": 5501
}
